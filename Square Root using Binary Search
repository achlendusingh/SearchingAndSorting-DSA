#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
cout<<"Enter the number whose sqrt you want too find"<<endl;
int n;
cin>>n;
int low=1;
int high=n;
int mid,ans;
int precision;
cout<<"Enter the precision you want"<<endl;
cin>>precision;
 double step=0.1;
while(low<=high)
{
    mid=low+(high-low)/2;
    if(mid*mid==n)
    {
        ans=mid;
        break;
    }
    else if((mid*mid)<n) {
        low=mid+1;
          ans=mid;
    }
    else 
    {
       high=mid-1;
    }
}
double finalAns=ans;
for (int i=1;i<=precision;i++)
{
    for(double j=finalAns;j*j<n;j+=step)
    {
        if(j*j<=n)
        {
            finalAns=j;
        }
    }
    step/=10;
}
cout<<"sqrt of "<<n<<" is: "<<finalAns;
    return 0;
}
