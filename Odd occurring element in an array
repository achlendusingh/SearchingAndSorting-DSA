#include <iostream>
#include <vector>
using namespace std;
int main() {
    // Write C++ code here
    int n;
  cout<<"Enter the number of elements array(Must be odd)"<<endl;
  cin>>n;
  vector <int> v;
  cout<<"Enter the elements"<<endl;
  for (int i=0;i<n;i++)
  {
      int a;
      cin>>a;
      v.push_back(a);
  }
  cout<<"The array is ";
  for (int i=0;i<n;i++)
  {
      cout<<v[i]<<" ";
  }
  cout<<endl;
  int low=0;
  int high=v.size()-1;
  int mid;
  while(low<=high)
  {
      mid=(low+high)/2;
      if(mid%2==0)
      {
          if(v[mid]==v[mid+1])
          {
              low+=2;
          }
          else if (v[mid]==v[mid-1])
          {
              high-=2;
          }
          else
          {
              cout<<"The answer is: "<<v[mid]<<endl;
              break;
          }
      }
      else
      {
          if(v[mid]==v[mid-1])
          {
              low+=1;
          }
          else
          {
              high-=1;
          }
      }
  }
    return 0;
}
